{
  "name": "swagger-enabled-api-server",
  "version": "1.0.0",
  "description": "A starter project for a swagger enabled api.",
  "main": "index.js",
  "scripts": {
    "test": "lab -v -L --lint-warnings-threshold 20 -C -D -c -t 80 ",
    "cover": "lab -r html -o coverage.html",
    "start": "node index.js",
    "start:dev": "pm2 start process.json --env dev ; npm run logs",
    "start:test": "npm run test ; pm2 start process.json --env test",
    "start:prod": "pm2 start process.json --env prod",
    "stop": "pm2 stop all",
    "delete": "pm2 delete all",
    "list": "pm2 list all",
    "logs": "pm2 logs",
    "docker": "docker build -t api-server . ; docker run -it -p 80:8000 api-server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "boom": "^4.3.0",
    "good": "^7.1.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.1.0",
    "hapi-swagger": "^7.7.0",
    "inert": "^4.1.0",
    "joi": "^10.3.1",
    "mongojs": "^2.4.0",
    "pm2": "^2.4.2",
    "vision": "^4.1.1"
  },
  "devDependencies": {
    "code": "^4.0.0",
    "eslint": "^3.18.0",
    "eslint-config-hapi": "^10.0.0",
    "eslint-plugin-hapi": "^4.0.0",
    "lab": "^13.0.1"
  }
}
